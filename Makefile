
 
	  
# Compiler Specific:
COMP=gcc
CFLAG=-c
WFLAG=-Wall
OFLAG=-o
testnum=0

# Program Specific:
SOURCES=	main.c		\
			dbpbe.c

OBJECTS=$(SOURCES:.c=.o)
HEADERS=$(SOURCES:.c=.h)
EXECUTABLE=DBPBE

# Start of Make:
Start: clean $(SOURCES) $(EXECUTABLE)

$(EXECUTABLE): $(OBJECTS) 
	$(COMP) $(OBJECTS) -lm $(OFLAG) $@ $(WFLAG)

.c.o:
	$(COMP) -D MODE=$(testnum) $(CFLAG) $< $(OFLAG) $@ $(WFLAG)

# Cleanup Routines:
clean:
	rm -f $(OBJECTS) $(EXECUTABLE)

